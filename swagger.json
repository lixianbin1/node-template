{  
  "openapi": "3.0.3",  
  "info": {  
    "title": "My API",  
    "version": "1.0.0"  
  },  
  "paths": {  
    "/users": {  
      "get": {  
        "summary": "获取用户列表",  
        "responses": {  
          "200": {  
            "description": "成功获取用户列表",  
            "content": {  
              "application/json": {  
                "schema": {  
                  "type": "array",  
                  "items": {  
                    "$ref": "#/components/schemas/User"  
                  }  
                }  
              }  
            }  
          }  
        }  
      },  
      "post": {  
        "summary": "创建新用户",  
        "requestBody": {  
          "content": {  
            "application/json": {  
              "schema": {  
                "$ref": "#/components/schemas/User"  
              }  
            }  
          }  
        },  
        "responses": {  
          "201": {  
            "description": "成功创建新用户"  
          }  
        }  
      }  
    }  
  },  
  "components": {  
    "schemas": {  
      "User": {  
        "type": "object",  
        "properties": {  
          "id": {  
            "type": "integer",  
            "description": "用户ID"  
          },  
          "username": {  
            "type": "string",  
            "description": "用户名"  
          },  
          "email": {  
            "type": "string",  
            "description": "用户邮箱"  
          }  
        }  
      }  
    }  
  }  
}